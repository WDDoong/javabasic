package inheritance;

public class Main02 {
	
	public static void main(String[] args) {
		Salaryman sl1 = new Salaryman();
		sl1.name = "최이박";
		sl1.age = 30;
		sl1.salary = 3000;
		
		Student s1 = new Student();
		s1.name = "김나박";
		s1.age = 20;
		s1.major = "경영학과";

		sl1.getSalaryInfo();
		System.out.println("--------------");
		s1.getStudentInfo();
		
	}

}
