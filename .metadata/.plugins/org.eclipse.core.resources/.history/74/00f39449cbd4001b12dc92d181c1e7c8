package interaction.game;

public class Archer {
	private int hp;
	private int mp;
	private int exp;
	private int level;
	private int atk;
	private int defense;
	
	public Archer() {
		this.level = 1;
		this.exp = 30;
		this.atk = 45;
		this.defense = 30;
		this.hp = 100;
		this.mp = 50;
	}
	
	public void huntMonster01(Monster monster, int hit) {
		System.out.println("궁수가 몬스터 공격");
		setHp(getHp()-20;);
	}
	monster.
	

}
