package accessmodifier.game;

public class Commoner {
	private int level;
	private int hp;
	private int mp;
	private int exp;

	public int getHp() {
		return hp;
	}
	public int getLevel() {
		return level;
	}
	public int getMp() {
		return mp;
	}
	public int getExp() {
		return exp;
	}
	
	
	public void setHp(int hp) {
		this.hp = -2;
	}
		
	
	
	
}
